{% extends 'base.twig' %}

{% block body %}
<div class="container">
<h1>User List</h1>
<table class="table table-striped dt">
 <thead>
 <tr>
     <th>id</th>
     <th>name</th>
     <th>email</th>
     <th>roles</th>
     <th>password</th>
     <th>createAt</th>
     <th>updatedAt</th>
    <th></th>
 </tr>
 </thead>
 <tbody>
 {% for user in users %}
 <tr>
     <td>{{users.id}}</td>
     <td>{{users.name}}</td>
     <td>{{users.email}}</td>
     <td>{{users.roles}}</td>
     <td>{{users.password}}</td>
     <td>{{users.createAt}}</td>
     <td>{{users.updatedAt}}</td>
    <td>
   <a href="/{{ record.path }}/{{ record.id }}/"><i class="fa fa-search"></i> </a>
   <a href="/{{ record.path }}/{{ record.id }}/edit"><i class="fa fa-edit"></i></a>
  </td>
 </tr>
 {% endfor %}
 </tbody>
</table>
</div>
{% endblock %}
